<?xml version="1.0" encoding="utf-8" ?>
<!--xlm 값의 형식 버전 1.0의 인코딩을 utf-8로 변환시킨다. -->
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<!--DOCTYPE 형태인 properties의 시스템에 url를 입력받아 dtd를 시스템을 만든다-->
<properties>
    <!--properties 데이터값들을 블럭으로 지정 한다-->
    <comment/>
<!-- 키 -->
        <entry key=" selectEmpByFamilyName">
            <!--들어올 키값에 selectEmpByFamilName이 들어오게 된다-->
<!-- 벨류 -->
            select
            *
            from
            employee
            where
            emp_name like concat(?,'%')
        </entry>


</properties>